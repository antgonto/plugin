<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <extension
         point="org.eclipse.ui.commands">
      <category
            description="Plugin commands"
            id="marker.commands.category"
            name="AVIB">
      </category>
      <command
            name="Sample Command"
            categoryId="marker.commands.category"
            id="marker.commands.sampleCommand">
      </command>
      <command
            categoryId="marker.commands.category"
            description="Disconnect to Server"
            id="marker.command.disconnect"
            name="Disconnect Command">
      </command>
      <command
            categoryId="marker.commands.category"
            description="Connect to Server"
            id="marker.command.connect"
            name="Connect Command">
      </command>
      <command
            categoryId="marker.commands.category"
            description="Send data to Server"
            id="marker.command.send"
            name="Send Command">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.handlers">
      <handler
            commandId="marker.commands.sampleCommand"
            class="marker.handlers.SampleHandler">
      </handler>
      <handler
            commandId="marker.command.connect">
      </handler>
   </extension>
   <extension
         point="org.eclipse.ui.bindings">
      <key
            commandId="marker.commands.sampleCommand"
            contextId="org.eclipse.ui.contexts.window"
            sequence="M1+6"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration">
      </key>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            locationURI="menu:org.eclipse.ui.main.menu?after=additions">
         <menu
               label="Sample Menu"
               mnemonic="M"
               id="marker.menus.sampleMenu">
            <command
                  commandId="marker.commands.sampleCommand"
                  mnemonic="S"
                  id="marker.menus.sampleCommand">
            </command>
         </menu>
      </menuContribution>
      <menuContribution
            locationURI="toolbar:org.eclipse.ui.main.toolbar?after=additions">
         <toolbar
               id="marker.toolbars.sampleToolbar">
            <command
                  commandId="marker.commands.sampleCommand"
                  icon="icons/sample.gif"
                  tooltip="Say hello world"
                  id="marker.toolbars.sampleCommand">
            </command>
         </toolbar>
      </menuContribution>
   </extension>
   <extension
         id="id1"
         point="org.eclipse.core.resources.markers">
   </extension>
   <extension
         point="org.eclipse.ui.popupMenus">
   </extension>
   <extension
         point="org.eclipse.ui.popupMenus">
   </extension>
   <extension
         point="org.eclipse.ui.editors.markerAnnotationSpecification">
      <specification
            annotationType="marker.specification1"
            includeOnPreferencePage="true">
      </specification>
   </extension>
   <extension
         point="org.eclipse.ui.editors.annotationTypes">
      <type
            name="marker.type1">
      </type>
   </extension>
   <extension
         point="org.eclipse.ui.editors.markerUpdaters">
      <updater
            class="marker.Updater1"
            id="marker.updater1">
      </updater>
   </extension>
   <extension
         point="org.eclipse.ui.decorators">
      <decorator
            id="marker.decorator1"
            label="label">
      </decorator>
   </extension>
   <extension
         point="org.eclipse.core.runtime.products">
   </extension>

</plugin>
